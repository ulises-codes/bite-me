{
  "version": "5.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ulises-codes/bite-me.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "types": "./bite-me.d.ts",
  "isLibrary": true,
  "files": [
    "dist",
    "snake",
    "offscreen"
  ],
  "browserslist": [
    "supports es6-module"
  ],
  "keywords": [
    "snake",
    "bite-me",
    "ulises himely",
    "react game",
    "html canvas",
    "offscreen canvas",
    "web workers"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "types": "tsc --project tsconfig.json",
    "build": "rm -rf dist && npm run types && parcel build --no-source-maps ./src/offscreen.tsx ./src/worker.ts ./src/snake.tsx --public-url ./",
    "start": "rm -rf dist && npm run types && parcel watch  ./src/offscreen.tsx ./src/worker.ts ./src/snake.tsx --no-cache --no-hmr --public-url ./"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "sideEffects": false,
  "name": "@ulises-codes/bite-me",
  "author": "Ulises Himely",
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@types/node": "^14.14.22",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "parcel-bundler": "^1.12.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.14",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "comlink": "^4.3.0"
  }
}
