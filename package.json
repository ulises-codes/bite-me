{
  "version": "6.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ulises-codes/bite-me.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "isLibrary": true,
  "files": [
    "dist",
    "snake",
    "offscreen",
    "worker"
  ],
  "browserslist": [
    "supports es6-module"
  ],
  "keywords": [
    "snake",
    "bite-me",
    "ulises himely",
    "react game",
    "html canvas",
    "offscreen canvas",
    "web workers"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack && npm run copy-types",
    "copy-types": "cp -r ./src/types ./dist/",
    "watch": "NODE_ENV=development webpack --watch && npm run copy-types"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "sideEffects": false,
  "name": "@ulises-codes/bite-me",
  "author": "Ulises Himely",
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@types/node": "^14.14.22",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "parcel-bundler": "^1.12.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.14",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "comlink": "^4.3.0"
  }
}
